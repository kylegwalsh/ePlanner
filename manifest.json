{
  "manifest_version": 2,

  "name": "ePlanner Extension",
  "description": "ePlanner extension for HCI Project",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ePlanner Extension"
  },
  "permissions": [
    "activeTab",
    "debugger",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}