{
  "manifest_version": 2,

  "name": "ePlanner Extension",
  "description": "ePlanner extension for HCI Project",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "ePlanner Extension"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "activeTab",
    "debugger",
    "storage",
    "alarms", 
    "notifications",
    "https://ajax.googleapis.com/"
  ],
  "minimum_chrome_version": "23"
}