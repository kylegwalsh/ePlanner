{
  "manifest_version": 2,

  "name": "Eplanner Extension",
  "description": "Eplanner extension, HCI Project",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.htm",
    "default_title": "Click here!"
  },
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["lib/js/jquery-3.1.1.min.js", "lib/js/angular.min.js", "lib/js/bootstrap.min.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}